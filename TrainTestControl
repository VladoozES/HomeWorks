import random


data_count = 102
data = [random.randint(0, 10000) for i in range(data_count)]
Controls = [data.pop(random.randint(0, len(data) - 1)) for i in range(2)]
train_count = int(len(data) * 0.8)
Train = []
for i in range(train_count):
    Train.append(data.pop(random.randint(0, len(data) - 1)))
Test = data.copy()

print(Train)
print(Test)
print(Controls)
